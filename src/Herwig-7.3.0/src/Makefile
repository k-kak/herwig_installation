		-rm -f ./$(DEPDIR)/HerwigCLI.Po
	-rm -f ./$(DEPDIR)/HerwigMain.Po
	-rm -f ./$(DEPDIR)/herwigopts.Po
	-rm -f Makefile
distclean-am: clean-am distclean-compile distclean-generic \
	distclean-tags

